declare {
  val fibo, out
  fibo = def $ (n) {
            if(n<2) {return 1} else {return fibo(n-1) + fibo(n-2)}
         }
  out = fibo(30)
  browse(out)
}

///////////////////////////////////////////////
Translates to
///////////////////////////////////////////////
declare local
   Fibo = fun{$ N}
            if(N<2) then 1 else {Fibo N-1} + {Fibo N-2} end
          end
   Out = {Fibo 30}
in
  {Browse Out}
end